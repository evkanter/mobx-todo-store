{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-undef": 0,
        "no-console": 0,
        "no-unused-vars": "warn",
        "react/prop-types": "warn"
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"]
}